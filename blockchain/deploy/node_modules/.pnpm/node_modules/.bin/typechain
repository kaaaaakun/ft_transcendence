#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/typechain@8.3.2_supports-color@7.2.0_typescript@5.8.3/node_modules/typechain/dist/cli/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/typechain@8.3.2_supports-color@7.2.0_typescript@5.8.3/node_modules/typechain/dist/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/typechain@8.3.2_supports-color@7.2.0_typescript@5.8.3/node_modules/typechain/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/typechain@8.3.2_supports-color@7.2.0_typescript@5.8.3/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/typechain@8.3.2_supports-color@7.2.0_typescript@5.8.3/node_modules/typechain/dist/cli/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/typechain@8.3.2_supports-color@7.2.0_typescript@5.8.3/node_modules/typechain/dist/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/typechain@8.3.2_supports-color@7.2.0_typescript@5.8.3/node_modules/typechain/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/typechain@8.3.2_supports-color@7.2.0_typescript@5.8.3/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typechain/dist/cli/cli.js" "$@"
else
  exec node  "$basedir/../typechain/dist/cli/cli.js" "$@"
fi
