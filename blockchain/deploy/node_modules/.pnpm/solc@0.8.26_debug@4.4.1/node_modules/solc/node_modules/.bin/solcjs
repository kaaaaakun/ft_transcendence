#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/solc@0.8.26_debug@4.4.1/node_modules/solc/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/solc@0.8.26_debug@4.4.1/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/solc@0.8.26_debug@4.4.1/node_modules/solc/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/solc@0.8.26_debug@4.4.1/node_modules:/private/tmp/ft_transcendence/blockchain/deploy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../solc.js" "$@"
else
  exec node  "$basedir/../../solc.js" "$@"
fi
